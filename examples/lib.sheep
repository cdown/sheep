# library function ideas

(function nth-tail (list n)
  (if (and list n)
    (nth-tail (tail list) (- n 1))
    list))

(function nth (list n)
  (head (nth-tail list n)))

(function reduce (op list)
  (function iter (acc pos)
    (if pos
      (iter (op acc (head pos))
            (tail pos))
      acc))
  (iter (op (nth list 0)
            (nth list 1))
        (nth-tail list 2)))
